				"source": [
					"df_date = (df.select(\r\n",
					"    'vendorID',\r\n",
					"    'tpepPickupDateTime',\r\n",
					"    'tpepDropoffDateTime',\r\n",
					"    'passengerCount',\r\n",
					"    'tripDistance',\r\n",
					"    'puLocationId',\r\n",
					"    'doLocationId',\r\n",
					"    'startLon',\r\n",
					"    'startLat',\r\n",
					"    'endLon',\r\n",
					"    'endLat',\r\n",
					"    'rateCodeId',\r\n",
					"    'storeAndFwdFlag',\r\n",
					"    'paymentType',\r\n",
					"    'fareAmount',\r\n",
					"    'extra',\r\n",
					"    'mtaTax',\r\n",
					"    'improvementSurcharge',\r\n",
					"    'tipAmount',\r\n",
					"    'tollsAmount',\r\n",
					"    'totalAmount',\r\n",
					"    'puYear',\r\n",
					"    lpad(col('puMonth').cast(StringType()), 2, '0').alias('puMonth'),\r\n",
					"    lpad(dayofmonth('tpepPickupDateTime'), 2, '0').alias('puDay')\r\n",
					"                    )\r\n",
					"            )\r\n",
					"\r\n",
					"display(df_date.limit(5))\r\n",
					"df_date.printSchema()"
				],
				"execution_count": 17
			},
